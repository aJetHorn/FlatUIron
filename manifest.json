{
	"manifest_version": 2,

	"name": "FlatUIron",
	"description": "Flattens colors on a webpage",
	"version": "1.0.0",

	"browser_action": {
		"default_icon": "images/iron1.png",
		"default_title": "FlatUIron"
	},
	"background":{
		"scripts":["script/background.js"]
	},
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	],
	"content_scripts": [ {
		"js": ["script/jquery-2.1.4.min.js", "script/content.js"],
		"matches": [ "http://*/*", "https://*/*"]
		}]
}